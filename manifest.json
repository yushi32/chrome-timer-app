{
	"name": "timer app",
	"description": "タイマーアプリ",
	"version": "0.0.1",
	"manifest_version": 3,
	"action": {
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "src/background.js",
		"type": "module"
	},
	"permissions": [
		"background",
		"storage"
	],
	"content_security_policy": {
    "extension_pages": "script-src 'self' ; object-src 'self'"
  }
}
